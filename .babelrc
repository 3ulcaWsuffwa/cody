{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%"
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "relay",
    "syntax-dynamic-import",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  }
}
